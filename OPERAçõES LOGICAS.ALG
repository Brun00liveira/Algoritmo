Algoritmo "Operações Logicas"

Var
// Seção de Declarações das variáveis 
   A, B, C: Inteiro

Inicio
// Seção de Comandos, procedimento, funções, operadores, etc... 
    A <- 3
    B <- 2
    C <- 5
    Escreval ("A>B",(A>B))
    Escreval ("B<=(A+B)", B<(A+B))
    Escreval ("C=(B%2)", C=(B%2))
    Escreval ("A<>(B^2)", A<>(B^2))
    

Fimalgoritmo